LazarusResources.Add('HelpAllgemein','HTML',[
  '<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">'#10'<HTML>'#10
  +'<HEAD>'#10#9'<META HTTP-EQUIV="CONTENT-TYPE" CONTENT="text/html; charset=ut'
